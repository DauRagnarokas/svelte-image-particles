{
  "name": "svelte-image-particles",
  "version": "0.0.1",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "npm run prepack",
    "prepare": "npm run prepack",
    "prepack": "svelte-kit sync && svelte-package && cpy 'src/lib/{scripts,shaders}/**/*' dist/",
    "package": "svelte-package",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "three": "^0.168.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "svelte": "^5.0.0",
    "vite": "^7.0.4",
    "typescript": "^5.3.2",
    "tailwindcss": "^4.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "cpy-cli": "^6.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0"
  },
  "keywords": [
    "svelte",
    "particles",
    "canvas",
    "image",
    "three.js",
    "shader"
  ]
}
